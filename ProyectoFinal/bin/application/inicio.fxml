<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.inicio">
   <top>
      <Label text="Enciclopedia" BorderPane.alignment="CENTER">
         <font>
            <Font name="Arial Rounded MT Bold" size="64.0" />
         </font>
      </Label>
   </top>
   <center>
      <Pane prefHeight="298.0" prefWidth="600.0" style="-fx-background-color: #ADD8E6;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btn_buscar" layoutX="14.0" layoutY="48.0" mnemonicParsing="false" onAction="#buscar" prefHeight="203.0" prefWidth="274.0" text="BUSCAR">
               <font>
                  <Font name="Arial Rounded MT Bold" size="36.0" />
               </font>
            </Button>
            <Button fx:id="btn_ingresar" layoutX="311.0" layoutY="49.0" mnemonicParsing="false" onAction="#ingresar" prefHeight="200.0" prefWidth="275.0" text="INGRESAR">
               <font>
                  <Font name="Arial Rounded MT Bold" size="36.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </center>
</BorderPane>
